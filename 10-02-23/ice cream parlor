public static List<Integer> icecreamParlor(int m, List<Integer> arr) {
     int a=arr.size();
     ArrayList<Integer> ar=new ArrayList<>();
     for(int i=0;i<a;i++){
         for(int j=i+1;j<a;j++){
             if(arr.get(i)+arr.get(j)==m){
                 ar.add(i+1);
                 ar.add(j+1);
             }
         }
     }
          return ar;
    }

}

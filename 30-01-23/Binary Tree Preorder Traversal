class Solution {
         ArrayList<Integer> a=new ArrayList<>();
         public void b(TreeNode root){
          if(root==null){
            return ;
          }
             a.add(root.val);
        b(root.left);
        b(root.right);
         }
    public List<Integer> preorderTraversal(TreeNode root) {
        
        
            b(root);
            return a;
        }
        
    }

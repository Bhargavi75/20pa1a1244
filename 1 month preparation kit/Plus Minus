 public static void plusMinus(List<Integer> arr) {
    double a=0;
    double b=0;
    double c=0;
    int n=arr.size();
    for(int i=0;i<n;i++){
       if(arr.get(i)>0){
        a=a+1;
    }
       else if(arr.get(i)<0){
        b=b+1;
    }
       else if(arr.get(i)==0){
        c=c+1;
    }
    }  
       double s=a/n;
       double t=b/n;
       double u=c/n; 
    
      System.out.println(s);
      System.out.println(t);
      System.out.println(u);
    
    }
    
}

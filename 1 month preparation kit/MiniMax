public static void miniMaxSum(List<Integer> arr) {
     Collections.sort(arr);
     long max=0;
     long min=0;
     int n=arr.size();
     for(int i=0;i<n-1;i++)
     {
      min+=arr.get(i);  
     }
     for(int i=1;i<n;i++)
     {
      max+=arr.get(i);  
     }
    System.out.println(min+" "+max);
    
    }
}

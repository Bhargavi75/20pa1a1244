 public static int diagonalDifference(List<List<Integer>> arr) {
    int d1=0;
    int d2=0;
    int n=arr.size();
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(i==j){
                d1+=arr.get(i).get(j);
            }
        }
    }
    Collections.reverse(arr);
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(i==j){
                d2+=arr.get(i).get(j);
            }
        }
    }
      return Math.abs(d1-d2);

    }

}
